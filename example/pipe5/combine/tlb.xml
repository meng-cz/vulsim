<?xml version="1.0" encoding="UTF-8"?>

<combine>

    <name>RV64SV48TLB</name>

    <config>
        <name>SetPow2</name>
        <ref>TLB.SetPow2</ref>
    </config>
    <config>
        <name>WayCount</name>
        <ref>TLB.WayCount</ref>
    </config>

    <regout>
        <name>ptw_req</name>
        <type>uint64</type>
    </regout>

    <regin>
        <name>ptw_resp</name>
        <type>array&lt;uint64,2&gt;</type>
    </regin>

    <storage>
        <name>cacheblock</name>
        <type>GenericLRUBlock&lt;uint64&gt;</type>
        <value>GenericLRUBlock&lt;uint64&gt;(SetPow2, WayCount)</value>
    </storage>

    <service>
        <name>translate</name>
        <arg>
            <name>vaddr</name>
            <type>uint64</type>
        </arg>
        <return>
            <name>paddr</name>
            <type>uint64</type>
        </return>
        <return>
            <name>error</name>
            <type>uint32</type>
        </return>
        <cppfunc>
            <file>RV64SV48TLB.cpp</file>
            <name>RV64SV48TLB_translate</name>
        </cppfunc>
    </service>

    <tick>
        <cppfunc>
            <file>RV64SV48TLB.cpp</file>
            <name>RV64SV48TLB_tick</name>
        </cppfunc>
    </tick>



</combine>
