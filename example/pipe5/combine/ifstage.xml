<?xml version="1.0" encoding="UTF-8"?>

<combine>

    <name>IFStage</name>

    <pipeout>
        <name>if_out</name>
        <type>IDData</type>
    </pipeout>

    <storagenext>
        <name>pc</name>
        <type>uint64</type>
    </storagenext>

    <request>
        <name>icache_read</name>
        <arg>
            <name>paddr</name>
            <type>uint64</type>
        </arg>
        <arg>
            <name>len</name>
            <type>uint8</type>
        </arg>
        <return>
            <name>data</name>
            <type>uint64</type>
        </return>
        <return>
            <name>error</name>
            <type>uint32</type>
        </return>
    </request>

    <request>
        <name>translate</name>
        <arg>
            <name>vaddr</name>
            <type>uint64</type>
        </arg>
        <return>
            <name>paddr</name>
            <type>uint64</type>
        </return>
        <return>
            <name>error</name>
            <type>uint32</type>
        </return>
    </request>

    <service>
        <name>redirect</name>
        <arg>
            <name>pc</name>
            <type>uint64</type>
        </arg>
        <cppfunc>
            <file>IFStage.cpp</file>
            <name>IFStage_redirect</name>
        </cppfunc>
    </service>

    <tick>
        <cppfunc>
            <file>IFStage.cpp</file>
            <name>IFStage_tick</name>
        </cppfunc>
    </tick>



</combine>
